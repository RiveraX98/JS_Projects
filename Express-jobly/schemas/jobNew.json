{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Intelligence analyst"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                77000
            ]
        },
        "equity": {
            "type": "integer",
            "default": 0,
            "title": "The equity Schema",
            "examples": [
                0
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "garner-michael"
            ]
        }
    },
    "examples": [{
        "title": "Intelligence analyst",
        "salary": 77000,
        "equity": 0,
        "companyHandle": "garner-michael"
    }]
}